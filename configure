$as_echo "error" >&6; };;
	       5|fatal) $as_echo "#define WITH_MSGLEVEL 5" >>confdefs.h
 { $as_echo "$as_me:${as_lineno-$LINENO}: result: fatal" >&5
$as_echo "fatal" >&6; };;
	       *) $as_echo "#define WITH_MSGLEVEL 0" >>confdefs.h
 { $as_echo "$as_me:${as_lineno-$LINENO}: result: debug" >&5
$as_echo "debug" >&6; };;
	       esac
else
  $as_echo "#define WITH_MSGLEVEL 0" >>confdefs.h
 { $as_echo "$as_me:${as_lineno-$LINENO}: result: debug" >&5
$as_echo "debug" >&6; }
fi


